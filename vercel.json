{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "GEE_EMAIL": "@geefire-gee-email",
      "GEE_PRIVATE_KEY": "@geefire-gee-private-key"
    },
    "functions": {
      "api": {
        "handler": "app.app",
        "memory": 512
      }
    }
  }
  